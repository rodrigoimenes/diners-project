  "main": "index.js",
  "scripts": {
    "test": "export $(cat .env | xargs) && yarn jest --watchAll --runInBand --coverage",
    "start": "ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
    "start:prod": "node dist/index",
    "start:migrate:prod": "prisma migrate deploy && npm run start:prod",
    "seed": "export $(cat .env | xargs) && npx prisma generate && npx prisma migrate dev && node prisma/seed.js",
    "restart:db": "docker-compose stop && docker-compose down && docker-compose up"
